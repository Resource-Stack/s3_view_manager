


<div class="page-wrapper">
        <div class="page-content--bge5">
            <div class="container">
                <div class="login-wrap">
                    <% if notice %>
                      <p class="alert alert-success"><%= notice %></p>
                    <% end %>
                    <% if alert %>
                      <p class="alert alert-danger"><%= alert %></p>
                    <% end %>
                    <div class="login-content">
                        <div class="login-logo">
                            <a href="#">
                                <%= image_tag "logo.png", title: "S3 file manager" %>
                            </a>
                        </div>
                        <div class="login-form">
                            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                                <div class="form-group">
                                    <label>Email Address</label>
                                    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: 'au-input au-input--full',placeholder: "Email" %>
                                    
                                </div>
                                <div class="form-group">
                                    <label>Password</label>
                                    <%= f.password_field :password, autocomplete: "current-password", class: "au-input au-input--full",placeholder: "Password"  %>
                                    
                                </div>
                                <div class="login-checkbox">
                                    <label>
                                        <%= f.check_box :remember_me %>
                                        Remember Me
                                    </label>
                                    <label>
                                        
                                        <%# link_to "Forgotten Password?", new_password_path(resource_name) %>
                                    </label>
                                </div>
                                <%= f.submit "Log in",class: "au-btn au-btn--block au-btn--green m-b-20" %>
                                
                                
                           <% end %>
                            <div class="register-link">
                                <!--<p>
                                    Don't you have account?
                                   
                                     <%# link_to "Sign Up Here", new_registration_path(resource_name) %>
                                </p>-->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>